pipeline {
    agent any
    environment{
        branchName= '${GIT_BRANCH}'
    }
    stages{
        stage('Source Checkout') {
            steps {
                //checkout github code
                git 'https://github.com/supersourabh/HallsProject.git'
            }
        }
        stage('starting server') {
            steps {
                script{
                    def randomValue = UUID.randomUUID().toString()
                    echo'building on branch ${env.branchName}'
                }
                docker 'build -t $branchName-randomValue .'
            }
        }
    }
}
